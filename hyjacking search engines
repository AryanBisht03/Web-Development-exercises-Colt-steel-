        <form action="https://www.google.co.in/search?">
        <div>
            <label for="google">Google</label>
            <input type="text" placeholder="search" id="google" name="q">
            <button>search</button>
        </div>
        </form>

        <form action="https://www.youtube.com/results?">
        <div>
            <span><label for="youtube">Youtube</label></span>
            <input type="text" placeholder="search" id="youtube" name="search_query">
            <button>search</button>
        </div>
        </form>
        <form action="https://www.reddit.com/search/?">
             <label for="Reddit">Reddit</label>
             <input type="text" placeholder="search" id="Reddit" name="q">
             <button>search</button>
        </form>
    </main>
